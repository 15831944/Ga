        <table id="_schedulePlanning_ux_Grid" cellpadding="0" cellspacing="0" border="0">
            <thead>
                <tr>
                    <th>年/月</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>請求出来高</td>
                </tr>
