@{
    ViewBag.Title = "A~J-导入";
    Layout = "~/Views/Shared/_EasyuiLayout.cshtml";
}

@section HeaderScript{
    <script src="~/Content/Areas/AreasCode030/szrl100/javaScript/businesscommand.js"></script>
}
@section HeaderStyles{
    <link href="~/Content/Areas/AreasCode030/szrl100/css/szrl100.css" rel="stylesheet" />
    <style type="text/css">
        table.form084-table tr {
            height: 40px;
        }

        table.form084-table td input {
            width: 250px;
        }
    </style>
}
@section Body{
    <div class="easyui-layout">
        <div data-options="region:'north', collapsible: false" style="height: 35px;">
            <div class="tree-search">
                <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-save" plain="true" id="aj_import" onclick="importExcel2()">确定</a>
                <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-cancel" plain="true" onclick="cancel2()">取消</a>
            </div>
        </div>
        <div data-options="region:'center'">
            <form id="form111_edit3" method="post" enctype="multipart/form-data" novalidate style="margin:0;padding:2px 50px">
                <div style="display:none;">
                    <input id="rl11101" type="hidden" name="rl11101" />
                </div>
                <table class="form084-table table-panel">
                    <thead>
                        <tr style="height: 1px;">
                            <th style="width: 80px;"></th>
                            <th style="width: 300px;"></th>
                            <th style="width: 80px;"></th>
                            <th style="width: 300px;"></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text" colspan="4">
                                <label>1、数据源为一个EXCEL文件，请选择一个合法的EXCEL文件（包括路径和文件名）</label>
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>文件名：</label>
                            </td>
                            <td class="form084-table-td2" colspan="3">
                                <input type="hidden" id="input_hd_filepath_edit3" name="ExcelFilePath" />
                                <input id="excelFilePath2" type="text" name="ExcelFilePath_2" class="easyui-filebox" required="required" style="width: 640px;" data-options="buttonText: '选择文件'" />
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text" colspan="4">
                                <label>2、在您选择的EXCEL文件中选择一个工作表</label>
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>工作表：</label>
                            </td>
                            <td class="form084-table-td2" colspan="3">
                                <input id="excelWorkbook2" type="text" required="required" editable="false" name="ExcelWookSheet" class="easyui-combobox" style="width: 640px;" data-options="valueField:'id',textField:'text'" />
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text" colspan="4">
                                <label>3、填写该工作表中的各字相对应的列名（如A、B等）</label>
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>目录代码：</label>
                            </td>
                            <td class="form084-table-td2">
                                <input id="rl08406_edit2" type="text" name="rl08402" class="easyui-textbox" required="required" data-title="一级目录" />
                            </td>
                            <td class="form084-table-td1 form084-table-text">
                                <label>一级目录：</label>
                            </td>
                            <td class="form084-table-td2">
                                <input id="rl08406_edit2" type="text" name="rl08403" class="easyui-textbox" required="required" data-title="二级目录" />
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>税込金額——計（税込）：</label>
                            </td>
                            <td class="form084-table-td2">
                                <input id="rl08406_edit2" type="text" name="rl08410" class="easyui-textbox" required="required" data-title="材料编码" />
                            </td>
                            <td class="form084-table-td1 form084-table-text">
                                <label>税率：</label>
                            </td>
                            <td class="form084-table-td2">
                                <input id="rl08406_edit2" type="text" name="rl08411" class="easyui-textbox" required="required" data-title="件品名及摘要" />
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>税抜金額——計（税抜）：</label>
                            </td>
                            <td class="form084-table-td2">
                                <input id="rl08406_edit2" type="text" name="rl08414" class="easyui-textbox" required="required" data-title="报价名称" />
                            </td>
                            <td class="form084-table-td1 form084-table-text">
                                <label></label>
                            </td>
                            <td class="form084-table-td2">
                            </td>
                        </tr>

                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text" colspan="4">
                                <label>4、填写该工作表中数据范围：如（A1:J10）</label>
                            </td>
                        </tr>
                        <tr class="form084-table-tr">
                            <td class="form084-table-td1 form084-table-text">
                                <label>数据范围：</label>
                            </td>
                            <td class="form084-table-td2" colspan="3">
                                <input id="dataRange_edit2" type="text" name="DataRange" class="easyui-textbox" style="width: 640px;" required="required" validType="excelDataRange" />
                            </td>
                        </tr>
                    </tbody>
                </table>
            </form>
        </div>
    </div>
}
