@{
    ViewBag.Title = "采购合同列表";
    Layout = "~/Views/Shared/_EasyuiLayout.cshtml";
}

@section HeaderScript{
    <script src="~/Content/Areas/AreasCode030/szrl100/javaScript/businesscommand.js"></script>
}

@section Body{
    <div data-options="region:'center'">
        <div class="easyui-layout">
            <div data-options="region:'center'">
                <div id="tt" class="easyui-tabs">
                    <div id="tb1" title="新规采购申请">
                        <div class="nano">
                            <div class="nano-content">
                                <div class="easyui-layout" style="width:100%;height:100%;">
                                    @Html.Partial("_Purchase1", null, this.ViewData)
                                </div>
                                </>
                            </div>
                        </div>
                        <div id="tb1" title="采购申请变更">
                            <div class="nano">
                                <div class="nano-content">
                                    @Html.Partial("_Purchase2", null, this.ViewData)
                                </div>
                            </div>
                        </div>
                        <div id="tb1" title="采购申请取消">
                            <div class="nano">
                                <div class="nano-content">
                                    @Html.Partial("_Purchase3", null, this.ViewData)
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
}
