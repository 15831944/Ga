        /// <summary>
        /// 模糊匹配排序
        /// </summary>
        /// <param name="s1">原数组</param>
        /// <param name="s2">排序数组</param>
        /// <returns></returns>
        private string[] Arry_short(string[] s1, string[] s2)
        {
            List<int> _key = new List<int>();
            for (int i = 0; i < s1.Length; i++)
            {
                for (int ii = 0; ii < s2.Length; ii++)
                {
                    if (s1[i].Contains(s2[ii]))
                    {
                        _key.Add(ii);
                        break;
                    }
                }
            }
            Array.Sort(_key.ToArray(), s1);
            return s1;
        }
