        /// </summary>
        /// <param name="versionnumber"></param>
        /// <param name="_changeType"></param>
        /// <returns></returns>
        private string GetLastVersion(string zuofanid, string versionnumber, string _changeType)
        {
            string last_versionnumber = string.Empty;
            string c_1 = versionnumber.Substring(0, 2);
            string c_2 = versionnumber.Substring(2, 2);
            string c_3 = versionnumber.Substring(4, 2);

            string c_1_1 = (Convert.ToInt32(c_1) - 1).ToString().PadLeft(2, '0');
            string c_2_1 = (Convert.ToInt32(c_2) - 1).ToString().PadLeft(2, '0');
            string c_3_1 = (Convert.ToInt32(c_3) - 1).ToString().PadLeft(2, '0');
            IQueryable<szrl082> _s = this._szrl082Repository.GetModels(x => x.rl08204.Equals(zuofanid));
            try
            {
                using (IDbContextScope scope = this._scopeFactory.CreateScope())
                {
                    switch (_changeType)
                    {
                        case "订正":
                            string c_3_2 = _s.Where(x => x.rl08203.Substring(0, 2).Equals(c_1_1)).Max(m => m.rl08203.Substring(4, 2));
                            last_versionnumber = c_1_1 + c_2 + c_3_2;
                            break;
                        case "改定":
                            string c_2_2 = _s.Where(x => x.rl08203.Substring(2, 2).Equals(c_2_1)).Max(m => m.rl08203.Substring(4, 2));
                            last_versionnumber = c_1 + c_2_1 + c_2_2;
                            break;
                        case "内部修正":
                            last_versionnumber = c_1 + c_2 + c_3_1;
                            break;
                        default:
                            break;
                    }
                }
            }
            catch (Exception)
            {

                throw;
            }
            return last_versionnumber;
        }
