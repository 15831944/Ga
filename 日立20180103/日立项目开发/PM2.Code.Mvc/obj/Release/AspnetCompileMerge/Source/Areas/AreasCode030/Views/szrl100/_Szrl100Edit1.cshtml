@using PM2.Code
@model PM2.Models.Code030.szrl100Model.szrl100

    <table class="form100-table table-panel">
        <thead>
            <tr style="height: 10px;">
                <th style="width: 160px;"></th>
                <th style="width: 170px;"></th>
                <th style="width: 120px;"></th>
                <th style="width: 170px;"></th>
                <th style="width: 180px;"></th>
                <th style="width: 170px;"></th>
            </tr>
        </thead>
        <tbody>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text"><label>企业编码：</label></td>
                <td class="form100-table-td2">
                    @*@Html.EditorFor(x => x.rl10002)*@
                    <input type="hidden" name="rl10001" id="rl10001" />
                    <input type="hidden" name="rl10042" id="rl10042" />
                    <input type="hidden" name="rl10002" id="rl10002" />
                    <input type="text" id="SupplierCode" name="SupplierCode" class="easyui-textbox" disabled="disabled" />
                </td>
                <td class="form100-table-td1 form100-table-text">G&G代码：</td>
                <td class="form100-table-td2">
                    @*@Html.EditorFor(x => x.rl10041)*@
                    <input type="text" id="rl10041" name="rl10041" class="easyui-textbox" />
                </td>
                <td class="form100-table-td1 form100-table-text">G&G代码企业关联与否：</td>
                <td class="form100-table-td2">
                    @*@Html.EditorFor(x => x.rl10004_list)*@
                    <select id="rl10004" class="easyui-combobox" name="rl10004" data-options="editable: false" >
                        <option value="0">不关联</option>
                        <option value="1">一般</option>
                        <option value="2">关联公司</option>
                        <option value="3">母公司</option>
                    </select>
                </td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">企业名：</td>
                <td class="form100-table-td2">
                    @Html.EditorFor(x => x.rl10003, new { dataoptions = "required: true" })
                </td>
                <td class="form100-table-td1 form100-table-text">曾用名：</td>
                <td class="form100-table-td2">
                    @Html.EditorFor(x => x.rl10005)
                </td>
                <td class="form100-table-td1 form100-table-text"></td>
                <td class="form100-table-td2"></td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">企业类型：</td>
                <td class="form100-table-td2" colspan="5" id="td_form100_rl10006">
                    <input type="checkbox" name="rl10006_1" value="工事分包商" id="rl10006_1" /><label>工事分包商</label>
