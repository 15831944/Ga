                    <input type="checkbox" name="rl10007_2" value="材料" /><label>材料</label>
                    <input type="checkbox" name="rl10007_3" value="工事" /><label>工事</label>
                    <input type="checkbox" name="rl10007_4" value="服务" /><label>服务</label>
                    <input type="checkbox" name="rl10007_5" value="设计" /><label>设计</label>
                </td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">产品类型：</td>
                <td class="form100-table-td2">
                    @Html.EditorFor(x => x.rl10008, new { dataoptions = "required: true" })
                </td>
                <td class="form100-table-td1 form100-table-text">联系地址：</td>
                <td class="form100-table-td2">
                    @Html.EditorFor(x => x.rl10009, new { dataoptions = "required: true" })
                </td>
                <td class="form100-table-td1 form100-table-text"></td>
                <td class="form100-table-td2"></td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">公司电话：</td>
                <td class="form100-table-td2">
                    <input type="text" name="rl10010" class="easyui-textbox" required="true" />
                </td>
                <td class="form100-table-td1 form100-table-text">公司传真：</td>
                <td class="form100-table-td2">
                    <input type="text" name="rl10011" class="easyui-textbox" required="true" />
                </td>
                <td class="form100-table-td1 form100-table-text"></td>
                <td class="form100-table-td2"></td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">联系人1：</td>
                <td class="form100-table-td2">
                    <input type="text" name="ContactPerson1" class="easyui-textbox" id="form100_input_person1" data-message="联系人1" required="true" />
                </td>
                <td class="form100-table-td1 form100-table-text">联系电话1：</td>
                <td class="form100-table-td2">
                    <input type="text" name="ContactPhone1" class="easyui-textbox" id="form100_input_phone1" data-message="联系电话1" required="true" />
                </td>
                <td class="form100-table-td1 form100-table-text">电子邮箱1：</td>
                <td class="form100-table-td2">
                    <input type="text" name="Email1" class="easyui-textbox" validType='email' id="form100_input_email1" data-message="电子邮箱1" required="true" />
                </td>
            </tr>
            <tr class="form100-table-tr">
                <td class="form100-table-td1 form100-table-text">联系人2：</td>
                <td class="form100-table-td2">
                    <input type="text" name="ContactPerson2" class="easyui-textbox" />
